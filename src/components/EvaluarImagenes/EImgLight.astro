---
import {type EvaImgLight }from '../../types/api_ImgLight'

const resImgLight = await fetch('http://192.168.8.171:8000/ImgIluminada/',{
    method:"POST",
    headers: {
        "Content-Type":"application/json",
    },
    body: JSON.stringify({
        "url":"https://th.bing.com/th/id/OIP.k4uGYhZPu6Q5EVRb0cFtJgHaFj?rs=1&pid=ImgDetMain"
    })
});
const{ docs: resultados } = await resImgLight.json() as EvaImgLight
---

